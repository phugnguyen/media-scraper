generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Media {
  id          Int      @id @default(autoincrement())
  url         String
  type        String
  metadata    Json
  metadataText String @default("")
  originUrl   String @default("")
  createdAt   DateTime @default(now())

  @@index([url])
  @@index([type])
  @@index([originUrl])
  @@index([metadataText])
}

